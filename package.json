{
  "name": "viewit",
  "version": "0.0.22",
  "description": "viewIt",
  "main": "index.js",
  "scripts": {
    "dist": "electron-builder build -p always",
    "run": "electron index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOU-TIL/viewIt.git"
  },
  "author": "SeoRii <studioSeoRii@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YOU-TIL/viewIt/issues"
  },
  "build": {
    "productName": "viewIt",
    "appId": "youtil.doit.viewit",
    "artifactName": "viewIt.${ext}",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "NSIS",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./res/logo.ico"
    },
    "nsis": {
      "createStartMenuShortcut": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "you-til",
        "repo": "viewIt"
      }
    ]
  },
  "homepage": "https://github.com/YOU-TIL/viewIt#readme",
  "dependencies": {
    "electron-settings": "^4.0.2",
    "chardet": "^1.2.1",
    "iconv-lite": "^0.6.2",
    "md5-file": "^5.0.0",
    "electron-updater": "^4.3.4"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0"
  }
}
